{
  "name": "enmaca/boui-font-manager",
  "description": "uxmaltech/backoffice-ui Compatible App to easily manage and edit your entire font library, track multiple versions, and create typography categories for your projects.",
  "license": "Private",
  "authors": [
    {
      "name": "Enrique Martinez",
      "email": "enmaca@hotmail.com",
      "role": "Technical Lead & Developer"
    }
  ],
  "autoload": {
    "files": [
      "src/Functions.php"
    ],
    "psr-4": {
      "Enmaca\\Backoffice\\FontManager\\": "src/",
      "Enmaca\\Backoffice\\FontManager\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests",
      "Database\\": "database"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Enmaca\\Backoffice\\FontManager\\FontManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2 || ^8.3",
    "livewire/livewire": "^3.3",
    "illuminate/console": "*",
    "illuminate/contracts": "*",
    "illuminate/view": "*",
    "illuminate/database": "*",
    "symfony/dom-crawler": "^7.0",
    "symfony/css-selector": "^7.0",
    "laravel/framework": "^10.35|^11.0|^12.0",
    "ext-pdo": "*",
    "doctrine/dbal": "^3.8",
    "opis/closure": "^3.6",
    "guzzlehttp/guzzle": "^7.4",
    "uxmaltech/core": "*",
    "uxmaltech/backoffice-ui": "dev-ADR-001",
    "uxmaltech/backend-cbq": "*",
    "dompdf/php-font-lib": "^1.0",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.19|^9.0",
    "phpunit/phpunit": "^10.5|^11.0",
    "laravel/pint": "^1.13"
  },
  "repositories": {
    "uxmaltech-core": {
      "type": "path",
      "url": "../../uxmaltech/core",
      "options": {
        "symlink": true
      }
    },
    "uxmaltech-backoffice-ui": {
      "type": "path",
      "url": "../../uxmaltech/backoffice-ui",
      "options": {
        "symlink": true
      }
    },
    "uxmaltech-backend-cbq": {
      "type": "path",
      "url": "../../uxmaltech/backend-cbq",
      "options": {
        "symlink": true
      }
    }
  }
}
